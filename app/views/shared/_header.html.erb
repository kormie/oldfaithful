  <p class="alert"><%= alert %></p>
  <% end %>
  <div id="header_links">
    <p><span class="white"><%= @welcome_message %></span>
      &bull;
      <%= link_to "Preferences", edit_user_registration_path, :class => "yellow" %></p>
      <p>
        <%= hide(link_to "Tour Jot Locker", "#") %><%= link_to "Feedback", "mailto:feedback@jotlocker.com" %>&bull;<%= link_to "Contact Us", "mailto:contact@jotlocker.com" %>&bull;<%= link_to "Log Out", destroy_user_session_path %>
      </p>
  </div> <!--header links-->
</div><!--header-->